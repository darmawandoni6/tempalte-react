{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "ignorePatterns": ["next.config.js", ".next/*", "node_modules/*"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
